extend vistemplate
block sidelist
	ul.off-canvas-list
		li
			label Team
		li
			select(id='TeamName')
				option(value='' selected='selected' disabled="disabled") Choose Team
				option(value='bar') bar
				option(value='scatter') scatter
		li
			label Player
		li
			select(id='PlayerName')
				option(value='' selected='selected' disabled="disabled") Choose Player
		li
			label Year
		li
			select(id='Year')
				option(value='' selected='selected' disabled="disabled") Choose Year
				option(value='2015') 2015
				option(value='2015') 2014
		li.b
			input.button(type='button', value='Apply', onclick='generateCourt()')
        
block scripts
	script(src='static/js/lib/three.min.js')
	script(src='static/js/lib/TextGeometry.js')
	script(src='static/js/lib/FontUtils.js')
	script(src='static/js/lib/helvetiker_regular.typeface.js')
	script(src='static/js/lib/OrbitControls.js')
	script(src='static/js/lib/lodash.min.js')
	script(src='static/js/lib/d3.min.js')
	script(src='static/js/lib/StereoEffect.js')
	script(src='static/js/nbaInit.js')
	script(src='static/js/vrDetection.js')
	link(rel='stylesheet', href='//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css')
	script(src='//code.jquery.com/ui/1.11.4/jquery-ui.js')
				
		
		
